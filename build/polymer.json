{
  "entrypoint": "index.html",
  "shell": "src\\my-auburn.html",
  "fragments": [
    "src/my-welcome.html",
    "src/my-facility-maintenance.html",
    "src/my-view404.html"
  ],
  "sources": [
    "src/**/*",
    "images/**/*",
    "bower.json",
    "index.html",
    "src/my-auburn.html",
    "src/my-welcome.html",
    "src/my-facility-maintenance.html",
    "src/my-view404.html"
  ],
  "extraDependencies": [
    "bower_components/webcomponentsjs/*.js",
    "manifest.json",
    "bower_components/webcomponentsjs/*.js"
  ],
  "builds": [
    {
      "name": "es5-minified",
      "bundle": false,
      "html": {
        "minify": true
      },
      "css": {
        "minify": true
      },
      "js": {
        "minify": true,
        "compile": true
      },
      "addServiceWorker": true
    }
  ],
  "lint": {
    "rules": [
      "polymer-2"
    ]
  },
  "moduleResolution": "node"
}